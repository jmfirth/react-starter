{
  "name": "react-starter",
  "license": "MIT",
  "version": "0.0.1",
  "description": "A simple React starter with my currently preferred architecture, tooling and settings.  This is organic and will change both constantly and drastically at my whim.",
  "private": true,
  "scripts": {
    "clearmodules": "rm -rf node_modules",
    "coffee": "npm run clearmodules && npm install && npm run dev",
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build:webpack-dev": "webpack --config webpack.config.dev.js",
    "build:webpack-dev-hot": "webpack --config webpack.config.dev-hot.js",
    "build": "npm run clean && npm run build:webpack",
    "build-dev": "npm run clean && npm run build:webpack-dev-hot",
    "dev": "set NODE_PATH='app' && node devServer.js",
    "lint": "eslint --ext .jsx --ext .js ./; exit 0",
    "test": "set NODE_PATH='app' && babel-node app/test/index.js | faucet; exit 0",
    "update": "updtr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmfirth/react-starter.git"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "Justin Firth <jmfirth@gmail.com>",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "eslint": "^2.3.0",
    "eslint-plugin-react": "^4.2.1",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "exports-loader": "^0.6.3",
    "express": "^4.13.3",
    "faucet": "0.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "lodash": "^4.5.0",
    "node-sass": "^3.4.2",
    "path": "^0.12.7",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "tape": "^4.5.1",
    "updtr": "^0.1.10",
    "urijs": "^1.17.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.10.0"
